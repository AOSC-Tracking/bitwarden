{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Main",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/build",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "args": ["."]
    },
    {
      "name": "Attach Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 8315,
      "webRoot": "${workspaceRoot}",
      "timeout": 30000
    }
  ]
}
